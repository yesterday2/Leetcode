直接暴力解决
class Solution {
    public int strStr(String haystack, String needle) {
        int N=haystack.length() ,L =needle.length() ;
        for(int i=0;i<N- L+1;i++)
        {
            if( haystack.substring(i,i+L).equals(needle)){
                return i;
            }
        }
        return -1;
    }
}
